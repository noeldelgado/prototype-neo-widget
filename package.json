{
  "private": true,
  "name": "prototype-neo-widget",
  "version": "0.0.1",
  "scripts": {
    "start": "python -m SimpleHTTPServer",
    "dev": "watch 'npm run build' src",
    "build": "rollup -c",
    "watch": "tarima -wdV",
    "compile": "tarima -fdV"
  },
  "author": "Noel Delgado <auhcsei@gmail.com> (http://pixelia.me)",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-jsx": "^1.2.1",
    "babel-preset-es2015-rollup": "^1.1.1",
    "postcss-cssnext": "^2.5.2",
    "postcss-import": "^8.1.2",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollup-plugin-postcss": "0.0.27",
    "tarima-cli": "gextech/tarima-cli#next",
    "watch": "^0.18.0"
  },
  "dependencies": {
    "basscss": "^8.0.1",
    "dom-delegator": "^13.1.0",
    "virtual-dom": "^2.1.1"
  },
  "tarima": {
    "plugins": [
      "${PWD}/_rollup.config.js"
    ],
    "filter": [
      "!**/{components,containers,data,lib}/**"
    ]
  }
}
